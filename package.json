{
  "name": "react-rainbow-ui",
  "author": "froyog",
  "version": "0.1.0",
  "private": false,
  "homepage": "https://rainbow.twtstudio.com/react",
  "description": "A React UI library blending Rainbow CSS and material design",
  "keywords": [
    "react",
    "react-components",
    "rainbow"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/froyog/react-rainbow.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/froyog/react-rainbow/issues"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "deepmerge": "^2.0.1",
    "prop-types": "^15.6.0",
    "react-jss": "^8.2.1",
    "react-transition-group": "^2.2.1"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-preval": "^1.6.3",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "^21.9.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "marked": "^0.3.16",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "^1.15.2",
    "prismjs": "^1.11.0",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "size-limit": "^0.14.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build:docs": "node scripts/build.js",
    "build:commonjs": "cross-env NODE_ENV=package BABEL_ENV=commonjs babel ./src --ignore *.spec.js --out-dir ./build/lib",
    "build:modules": "cross-env NODE_ENV=package BABEL_ENV=modules babel ./src --ignore *.spec.js --out-dir ./build/es",
    "build:copyFiles": "node internals/copyFiles.js",
    "build": "yarn clean & yarn build:commonjs & yarn build:modules & yarn build:copyFiles",
    "clean": "rimraf ./build",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint --ext .jsx,.js src --fix && eslint --ext .jsx .js docs/src --fix",
    "size": "size-limit",
    "size:why": "size-limit --why"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/internals/test/setup.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
